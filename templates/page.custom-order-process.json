{"sections":{"main":{"type":"main-page","disabled":true,"settings":{"subheading":"","subheading_size":3,"show_title":true,"heading_size":8,"show_content":true,"text_size":4,"align_text":"text-left","width":"wrapper","bg_color":"","color":"","padding_top":50,"padding_bottom":50}},"double-first":{"type":"section-double","blocks":{"double-heading":{"type":"heading","settings":{"title":"Custom-Made Engagement Rings in Seattle","text_size":8,"padding_bottom":16}},"double-text":{"type":"text","settings":{"text":"<p>Looking to buy custom made <a href=\"https:\/\/www.valeriemadison.com\/collections\/engagement-rings\" title=\"https:\/\/www.valeriemadison.com\/collections\/engagement-rings\">engagement rings online<\/a> or custom wedding rings in Seattle? Explore our collection and start designing your own engagement ring today. Whether you prefer yellow gold, white gold, or lab grown diamonds, we have the perfect diamond shape and clarity to suit your style. Visit us for custom engagement rings in Seattle, and let us create the engagement ring of your dreams.<\/p>","text_size":4,"text_style":"font-body","padding_bottom":16}},"double-buttons":{"type":"buttons","settings":{"button_text":"View Our Past Custom Orders","button_url":"https:\/\/www.valeriemadison.com\/collections\/gallery-of-past-custom-orders","button_type":"btn--primary","button_size":"","button_style":"btn--outline","show_arrow":false,"padding_bottom":16}},"buttons_Vz8Kc6":{"type":"buttons","settings":{"button_text":"Book an in-person or virtual  appointment","button_url":"https:\/\/calendly.com\/valeriemadison","button_type":"btn--secondary","button_size":"","button_style":"btn--solid","show_arrow":false,"padding_bottom":16}}},"block_order":["double-heading","double-text","double-buttons","buttons_Vz8Kc6"],"settings":{"layout":"","height":"screen-height-three-quarters","mobile_height":"screen-height-one-half--mobile","width":"wrapper--full","image_1":"shopify:\/\/shop_images\/VMSummer2024-1131.jpg","image_widt